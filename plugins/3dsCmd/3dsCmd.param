[VRaySpawnerExistingProcess]
Type=enum
Category=V-Ray DBR Process Handling
Label=Handle Existing Spawner Process
CategoryOrder=4
CategoryIndex=0
Items=Fail On Existing Process;Kill On Existing Process
Description=Either FAIL on existing V-Ray Spawner process or KILL the existing V-Ray Spawner process if already running.
Default=Fail On Existing Process

[VRayDBRDynamicStart]
Category=V-Ray DBR Off-Load Rendering
Label=Dynamic Start
CategoryOrder=5
CategoryIndex=0
Type=Boolean
Description=If enabled, a V-Ray DBR off-load job will start rendering immediately after TaskId=0 (master machine) has started rendering. With V-Ray v3.2 (min SP2) onwards, Slaves dequeuing tasks of this job will start participating in the ongoing render when V-Ray re-scans it's *.cfg file on the master machine. Default: True.
Default=true

[VRayDBRPortRange]
Category=V-Ray DBR Off-Load Rendering
Label=V-Ray Port range
CategoryOrder=5
CategoryIndex=1
Type=string
Description=The port number (20204) or range (20204-20212) that the master machine will use to communicate to the V-Ray Spawner over TCP/IP. Default: 20204.
Default=20204

[VRayDBRUseLocalMachine]
Category=V-Ray DBR Off-Load Rendering
Label=Use local host
CategoryOrder=5
CategoryIndex=2
Type=Boolean
Description=When enabled, the master TaskId=0 machine (the Slave from which initiates the DR off-load render) will also take part in the rendering calculations. If disabled, this Deadline Slave only organizes the DR process, sending rendering tasks to the Deadline Slaves. Default: True.
Default=true

[VRayDBRTransferMissingAssets]
Category=V-Ray DBR Off-Load Rendering
Label=Transfer missing assets
CategoryOrder=5
CategoryIndex=3
Type=Boolean
Description=This option allows Deadline Slaves to automatically download missing rendering assets from the master machine. This includes textures, V-Ray proxy files, IES profiles, GI solution caches (irradiance map, light cache), X-Ref scenes etc. Default: False.
Default=false

[VRayDBRUseCachedAssets]
Category=V-Ray DBR Off-Load Rendering
Label=Use cached assets
CategoryOrder=5
CategoryIndex=4
Type=Boolean
Description=This option tells the Deadline Slaves to keep the downloaded rendering assets between different DR sessions. Default: False.
Default=false

[VRayDBRCacheLimitType]
Category=V-Ray DBR Off-Load Rendering
Label=Cache limit type
CategoryOrder=5
CategoryIndex=5
Type=enum
Values=None;Age (hours);Size (GB)
Description=Allows you to specify under what conditions the assets cache is cleaned up. The possible values are: None, Age (hours), Size (GB). Default: None.
Default=None

[VRayDBRCacheLimit]
Category=V-Ray DBR Off-Load Rendering
Label=Cache limit value
CategoryOrder=5
CategoryIndex=6
Type=float
Minimum=0.0
Maximum=1000000.0
DecimalPlaces=1
Increment=0.1
Description=The specific value of the limit for the asset cache. This is either hours or GB, depending on the value of Cache limit type. Min:0.0, Max:1000000.0. Default: 100.0.
Default=100.0

[VRayRTDBRDynamicStart]
Category=V-Ray RT DBR Off-Load Rendering
Label=Dynamic RT Start
CategoryOrder=6
CategoryIndex=0
Type=Boolean
Description=If enabled, a V-Ray RT DBR off-load job will start rendering immediately after TaskId=0 (master machine) has started rendering. With V-Ray v3.2 (min SP2) onwards, Slaves dequeuing tasks of this job will start participating in the ongoing render when V-Ray RT re-scans it's *.cfg file on the master machine. Default: True.
Default=true

[VRayRTDBRPortRange]
Category=V-Ray RT DBR Off-Load Rendering
Label=V-Ray RT Port range
CategoryOrder=6
CategoryIndex=1
Type=string
Description=The port number (20206) or range (20206-20212) that the master machine will use to communicate to the V-Ray RT Spawner over TCP/IP. Default: 20206.
Default=20206

[VRayRTDBRUseLocalMachine]
Category=V-Ray RT DBR Off-Load Rendering
Label=Auto-start local Slave
CategoryOrder=6
CategoryIndex=2
Type=Boolean
Description=When enabled, the master TaskId=0 machine (the Slave from which initiates the V-Ray RT DR off-load render) will also take part in the rendering calculations. If disabled, this Deadline Slave only organizes the V-Ray RT DR process, sending rendering tasks to the Deadline Slaves. Default: True.
Default=true
